package com.salesianostrina.dam.MainController;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;

@Controller
public class MainController {
	
	@GetMapping("/una")
	public String index() {
		return"index";
	}
	
	@GetMapping("/otra")
	public String pie() {
		return"pie";
	}

}
